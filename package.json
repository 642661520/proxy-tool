{
  "name": "proxy-tool",
  "version": "1.0.0",
  "description": "代理工具",
  "scripts": {
    "vite": "vite",
    "tsc": "cd electron && tsc",
    "electron:dev": "yarn tsc && wait-on tcp:5173 && chcp 65001 && cross-env NODE_ENV=development electron .",
    "electron:prd": "yarn tsc && chcp 65001 && electron .",
    "start": "concurrently -k \"vite\" \"yarn electron:dev\"",
    "watch:dev": "concurrently -k \"vite\" \"nodemon --watch ./electron/main.ts --exec yarn electron:dev\"",
    "watch:prd": "concurrently -k \"vite build --mode watch\" \"nodemon --watch ./electron/main.ts --exec yarn electron:prd\"",
    "build": "yarn tsc && vue-tsc --noEmit && vite build && chcp 65001 && electron-builder"
  },
  "main": "dist_electron/main.js",
  "author": "Final Summer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "http-proxy": "^1.18.1",
    "keyv": "^4.5.2",
    "naive-ui": "^2.30.8",
    "vue": "^3.2.25",
    "vue-i18n": "9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.9",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "21.2.2",
    "electron-builder": "^23.1.0",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "nodemon": "^2.0.20",
    "sass": "^1.43.4",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.13",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.9",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.github.com/642661520.electron-proxy",
    "productName": "端口代理工具",
    "copyright": "Copyright © 2022 <Final Summer>",
    "win": {
      "icon": "./src/assets/logo.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "extraResources": {
      "from": "./resource",
      "to": "resource"
    }
  }
}
